.gmaps-popup
  .gmaps-popup-image
    %img{ width: 50, height: 50, alt: "", src: "{{user.image}}" }
  .gmaps-popup-text
    %p.gmaps-popup-p-title
      %a{ href: "{{url}}" }
        {{start_address}}
        %br
        {{end_address}}
    %p.gmaps-popup-p-content
      {{user.name}}
  .gmaps-popup-div-details
    {{content}}
